<script type="text/javascript">
function changeby(str,change)
{
	
	var i=0;
	var num='';
	while(str[i]!='p')
	{
		num+=str[i];
		i+=1;
	}
	num=Number(num);
	num+=change;
	num+="px";
	return num;
}
function adjustheightofpage(newheight)
{
	var footer=document.getElementById('footer');
	var layer_main=document.getElementById('layer_main');
	layer_main.style.height=newheight;
	footer.style.top=changeby(newheight,-37);
}
</script>